                        <li><a class="popup-with-zoom-anim" href="#test-search"><i class="fa fa-search"></i></a></li>
                    </ul>
                </div>
            </div>
        </div>
        <div class="main_menu_area">
            <div class="container">
                <nav class="navbar navbar-expand-lg navbar-light bg-light">
                    <a class="navbar-brand" href="index.html">
                        <img src="~/TemplateShop/img/logo.png" alt="">
                        <img src="~/TemplateShop/img/logo-2.png" alt="">
                    </a>
                    <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarSupportedContent" aria-controls="navbarSupportedContent" aria-expanded="false" aria-label="Toggle navigation">
                        <span class="my_toggle_menu">
                            <span></span>
                            <span></span>
                            <span></span>
                        </span>
                    </button>
                    <div class="collapse navbar-collapse" id="navbarSupportedContent">
                        <ul class="navbar-nav mr-auto">
                            <li><a href="/Home/index">Home</a></li>
                            <li><a href="/Cake">Our Cakes</a></li>
                            <li><a href="/Menu">Menu</a></li>
                            <li class="dropdown submenu">
                                <a class="dropdown-toggle" data-toggle="dropdown" href="#" role="button" aria-haspopup="true" aria-expanded="false">About Us</a>
                                <ul class="dropdown-menu">
                                    <li><a href="/AboutUs/AboutUs">About Us</a></li>
                                    <li><a href="/AboutUs/Ourchefs">Our Chefs</a></li>
                                    <li class="active"><a href="/AboutUs/Testimonials">Testimonials</a></li>
                                </ul>
                            </li>
                        </ul>
                        <ul class="navbar-nav justify-content-end">

                            <li class="dropdown submenu">
                                <a class="dropdown-toggle" data-toggle="dropdown" href="#" role="button" aria-haspopup="true" aria-expanded="false">Shop</a>
                                <ul class="dropdown-menu">
                                    <li><a href="/Cake/Shop">Main shop</a></li>

                                    <li><a href="/Cart/Cart">Cart Page</a></li>
                                    <li><a href="/Cart/Checkout">Checkout Page</a></li>
                                </ul>
                            </li>
                            @if (ViewBag.UserName != null)
                            {
                                <li class="dropdown submenu">
                                    <a class="dropdown-toggle" data-toggle="dropdown" href="/Home/OderStatus" role="button" aria-haspopup="true" aria-expanded="false">Me</a>
                                    <ul class="dropdown-menu">
                                        @if (ViewBag.UserId == 1)
                                        {
                                            <li><a href="/Home/Admin">Orders by Client</a></li>

                                        }
                                        <li> <a href="/Home/OderStatus">My Order</a></li>
                                        <li> <a href="/AboutUs/Ourchefs">My Information</a></li>
                                        <li> <a href="@Url.Action("ChangePass", "User")">Change PassWord</a>
                                        <li> <a href="@Url.Action("Login", "User")">Logout</a></li>
                                    </ul>
                                </li>
                            }
                        </ul>
                    </div>
                </nav>
            </div>
        </div>
